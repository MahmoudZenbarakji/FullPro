generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"  // or "mysql" / "sqlite" / "mongodb"
  url      = env("DATABASE_URL")
}

model Course {
  id    Int    @id @default(autoincrement())
  title String @unique
  description  String?
  price Int
  Image String?
  dateCreated DateTime @default(now())
}
